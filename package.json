{
	"name": "chatbotbuilder",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start:migrate": "npx prisma migrate dev && npm run build",
		"start:prod": "vite build",
		"migrate:dev": "prisma migrate dev --preview-feature",
		"migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
		"migrate:reset": "prisma migrate reset --preview-feature",
		"migrate:deploy": "npx prisma migrate deploy --preview-feature",
		"migrate:status": "npx prisma migrate status --preview-feature",
		"migrate:resolve": "npx prisma migrate resolve --preview-feature",
		"prisma:studio": "npx prisma studio",
		"prisma:generate": "npx prisma generate",
		"prisma:generate:watch": "npx prisma generate --watch",
		"start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
		"seed": "prisma db seed",
		"postinstall": "npm run prisma:generate",
		"docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
		"docker:db": "docker-compose -f docker-compose.db.yml up -d",
		"docker:seed": "docker exec -it nest-api npm run seed",
		"docker": "docker-compose up -d",
		"docker:build": "docker-compose build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.12.2",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite-svelte-icons": "^1.5.0",
		"lucia": "^3.1.1",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.11.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@popperjs/core": "^2.11.8",
		"@prisma/client": "^5.11.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.24",
		"js-yaml": "^4.1.0",
		"node-cmd": "^5.0.0",
		"node-run-cmd": "^1.0.1",
		"oslo": "^1.2.0",
		"svelte-fa": "^4.0.2",
		"svelte-notifications": "^0.9.98",
		"tailwind-merge": "^2.2.2",
		"zod": "^3.22.4"
	}
}
